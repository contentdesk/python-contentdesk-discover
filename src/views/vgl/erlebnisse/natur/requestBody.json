{
    "name": "VGL Erlebnisse - Natur",
    "description": "VGL Erlebnisse - Natur Objekte",
    "scheduleStrategy": "Daily",
    "searchRequest": {
            "project": [
                "{{project}}"
            ],
            "facets": [
                {
                    "name": "containedInPlace/id",
                    "responseNames": {
                        "de": "Ferienregion",
                        "en": "vacation area"
                    },
                    "filterValues": ["ds_braunwald_toedigebiet", "ds_elm_sernftal", "ds_glarus_kloental", "ds_glarusnord_walensee", "ds_grosstal_mettmen"],
                    "orderBy": "name",
                    "orderDirection": "asc"
                },
                {
                    "name": "leafType",
                    "responseNames": {
                        "de": "Natur-Typ",
                        "en": "nature type"
                    },
                    "orderBy": "name",
                    "orderDirection": "asc",
                    "count": 100
                },
                {
                    "name": "tag",
                    "additionalType": ["seasonality"],
                    "responseNames": {
                        "de": "Saisonalit√§t",
                        "en": "Seasonality"
                    }
                },
